{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "tutorials": {
      "type": "array",
      "title": "tutorials",
      "items": {
        "title": "id",
        "type": "object",
        "properties": {
          "key": {
            "type": "string",
            "pattern": ".{1,}"
          },
          "value": {
            "type": "object",
            "properties": {
              "max_step": {
                "title": "max_step",
                "type": "number",
                "minimum": 0
              },
              "start_step": {
                "title": "start_step",
                "type": "number",
                "minimum": 0
              },
              "additional_properties": {
                "type": "array",
                "title": "additional_properties",
                "items": {
                  "type": "object",
                  "properties": {
                    "key": {
                      "type": "string",
                      "pattern": ".{1,}"
                    },
                    "value": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
