{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "properties": {
    "initialize_user": {
      "type": "object",
      "properties": {
        "currencies": {
          "type": "object",
          "patternProperties": {
            ".{1,}": {
              "type": "number",
              "minimum": 0
            }
          }
        },
        "items": {
          "type": "object",
          "patternProperties": {
            ".{1,}": {
              "type": "number",
              "minimum": 0
            }
          }
        }
      }
    },
    "donations": {
      "type": "object",
      "patternProperties": {
        ".{1,}": {
          "type": "object",
          "properties": {
            "cost": {
              "type": "object",
              "properties": {
                "currencies": {
                  "type": "object",
                  "patternProperties": {
                    ".{1,}": {
                      "type": "number",
                      "minimum": 0
                    }
                  }
                },
                "items": {
                  "type": "object",
                  "patternProperties": {
                    ".{1,}": {
                      "type": "number",
                      "minimum": 0
                    }
                  }
                }
              }
            },
            "count": {
              "type": "number",
              "minimum": 0
            },
            "description": {
              "type": "string",
              "pattern": ".*"
            },
            "name": {
              "type": "string",
              "pattern": ".{1,}"
            },
            "max_count": {
              "type": "number",
              "minimum": 0
            },
            "recipient_reward": {
              "$ref": "Hiro-Rewards"
            },
            "contributor_reward": {
              "$ref": "Hiro-Rewards"
            },
            "user_contribution_max_count": {
              "type": "number",
              "minimum": 0
            },
            "additional_properties": {
              "patternProperties": {
                ".{1,}": {
                  "type": "string"
                }
              },
              "type": "object"
            }
          },
          "required": [ "name", "max_count" ]
        }
      }
    },
    "store_items": {
      "type": "object",
      "patternProperties": {
        ".{1,}": {
          "type": "object",
          "properties": {
            "category": {
              "type": "string",
              "pattern": ".{1,}"
            },
            "cost": {
              "type": "object",
              "properties": {
                "currencies": {
                  "type": "object",
                  "patternProperties": {
                    ".{1,}": {
                      "type": "number",
                      "minimum": 0
                    }
                  }
                },
                "sku": {
                  "type": "string",
                  "pattern": ".{1,}"
                }
              }
            },
            "description": {
              "type": "string",
              "pattern": ".*"
            },
            "name": {
              "type": "string",
              "pattern": ".{1,}"
            },
            "reward": {
              "$ref": "Hiro-Rewards"
            },
            "additional_properties": {
              "patternProperties": {
                ".{1,}": {
                  "type": "string"
                }
              },
              "type": "object"
            },
            "disabled": {
              "type": "boolean"
            }
          },
          "required": [ "name", "category" ]
        }
      }
    },
    "placements": {
      "type": "object",
      "patternProperties": {
        ".{1,}": {
          "type": "object",
          "properties": {
            "reward": {
              "$ref": "Hiro-Rewards"
            },
            "additional_properties": {
              "patternProperties": {
                ".{1,}": {
                  "type": "string"
                }
              },
              "type": "object"
            }
          }
        }
      }
    }
  },
  "type": "object"
}
